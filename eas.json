{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview-device": {
      "extends": "preview",
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "standalone-ios": {
      "distribution": "store",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium",
        "buildConfiguration": "Release"
      },
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "kevsjolinsched@gmail.com",
        "ascAppId": "6475963217",
        "appleTeamId": "KKUW8X6QXT"
      }
    }
  }
}
{
  "expo": {
    "name": "Kick the Nic",
    "slug": "kick-the-nic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#1a1a1a",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kickthenic.app",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "This app does not track you across apps and websites."
      },
      "appStoreUrl": "https://apps.apple.com/app/kick-the-nic/id6475963217",
      "config": {
        "usesNonExemptEncryption": false
      },
      "requireFullScreen": true
    },
    "android": {
      "package": "com.kickthenic.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#1a1a1a"
        }
      ],
      [
        "expo-background-fetch",
        {
          "netCacheDirectory": "net_cache"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b93fefea-74e5-4445-8a74-d19a703ec615"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "jncojeans",
    "privacy": "unlisted",
    "description": "An app to help you quit nicotine addiction",
    "githubUrl": "https://github.com/jncojeans/kickTheNic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    }
  }
}
